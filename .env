# Backend API Configuration
SPRING_BOOT_API_URL=http://localhost:8080/api
WEBSOCKET_URL=ws://localhost:8080/ws

# HIGH PERFORMANCE Recognition Settings
RECOGNITION_THRESHOLD=0.40
DETECTION_THRESHOLD=0.35
FRAME_PROCESSING_INTERVAL=3
MAX_CONCURRENT_CAMERAS=6

# OPTIMIZED Performance Settings for 15-30 FPS
PROCESSING_WIDTH=416
PROCESSING_HEIGHT=312
MAX_FACES_PER_FRAME=8

# Camera Performance Settings
CAMERA_BUFFER_SIZE=1
CAMERA_FPS=30

# Cache Settings
EMBEDDING_CACHE_DIR=data/embeddings
FACE_DB_PATH=data/faces_db.json
CACHE_EXPIRE_HOURS=24

# Logging
LOG_LEVEL=INFO
LOG_DIR=data/logs

# RTSP Settings - Optimized for low latency
RTSP_TIMEOUT=3000
HLS_SEGMENT_DURATION=1

# UI Settings
WINDOW_WIDTH=1400
WINDOW_HEIGHT=900

# InsightFace Settings - Use buffalo_s for speed
INSIGHTFACE_MODEL=buffalo_s

# Backend Authentication
BACKEND_USERNAME=admin
BACKEND_PASSWORD=admin@123
AUTO_LOGIN=true

# Login settings
LOGIN_RETRY_ATTEMPTS=3
LOGIN_RETRY_DELAY=2

# HIGH FPS OPTIMIZATION SETTINGS
ENABLE_PARALLEL_PROCESSING=true
MAX_WORKER_THREADS=4
ENABLE_QUALITY_FILTER=false
MIN_QUALITY_SCORE=0.15
MIN_BLUR_SCORE=10.0

# Tracking Settings - Optimized for speed
TRACK_BUFFER_SIZE=15
MIN_TRACK_LENGTH=3
TRACK_MATCH_THRESHOLD=0.5
STABLE_TRACK_THRESHOLD=0.75

# Brightness settings
MIN_BRIGHTNESS=20
MAX_BRIGHTNESS=250

# NEW: Webcam Support
ENABLE_WEBCAM_OPTION=true
WEBCAM_PROCESSING_WIDTH=320
WEBCAM_PROCESSING_HEIGHT=240
WEBCAM_FPS_TARGET=15

# Performance Mode (speed/balanced/accuracy)
PERFORMANCE_MODE=speed

# High FPS Mode (set to 1 for maximum speed optimization)
HIGH_FPS_MODE=1

# Debug settings
DEBUG_CONFIG=0